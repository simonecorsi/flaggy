{
  "name": "flaggy",
  "version": "1.5.0",
  "description": "Zero dependency command flag generator from a simple javascript object",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov  --reporter=text-summary ava",
    "prepare": "node prepare.js || echo 'Skipping prepare'"
  },
  "bugs": {
    "url": "https://github.com/simonecorsi/flaggy/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "nodejs",
    "npm",
    "javascript",
    "stringify",
    "argv",
    "opt",
    "flag",
    "command",
    "parser",
    "option parser",
    "options"
  ],
  "homepage": "https://github.com/simonecorsi/flaggy",
  "author": "Simone Corsi <simonecorsi.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@saithodev/semantic-release-backmerge": "^3.1.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "ava": "^6.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^15.2.0",
    "markdown-toc": "^1.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0"
  }
}
